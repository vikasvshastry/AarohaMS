<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aaroha.aaroha">

    <application
        android:name=".Aaroha"
        android:allowBackup="true"
        android:icon="@drawable/flower"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogBeforeMainActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/for_login" />
        <activity
            android:name=".RegBeforeMainActivity"
            android:parentActivityName=".LogBeforeMainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/for_login" />
        <activity
            android:name=".FrameActivity"
            android:label="@string/title_activity_frame"
            android:theme="@style/MyMaterialTheme"/>
        <activity android:name=".NewNotificationActivity" />
    </application>

</manifest>